{
  "usesTypeScript": true,
  "usesCssModule": false,
  "cssPreprocessor": "css",
  "testLibrary": "None",
  "component": {
    "default": {
      "path": "src/components",
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false,
      "customTemplates": {
        "component": "templates/TemplateName.component.tsx"
      }
    },
    "screen": {
      "path": "src/screens",
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false,
      "customTemplates": {
        "component": "templates/TemplateName.screen.tsx"
      }
    },
    "navigator": {
      "path": "src/navigation",
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false,
      "withType": true,
      "customTemplates": {
        "component": "templates/TemplateName.navigator.tsx",
        "type": "templates/TemplateName.types.ts"
      }
    }
  }
}
